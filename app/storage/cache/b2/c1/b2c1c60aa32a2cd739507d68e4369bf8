"blog_description";N;s:8:"blog_url";s:24:"http://stateofmind13.com";s:11:"blog_author";s:10:"Elie Fares";s:28:"blog_author_twitter_username";s:9:"eliefares";s:13:"blog_rss_feed";s:30:"http://stateofmind13.com/feed/";s:9:"blog_tags";s:17:"politics, society";s:2:"id";i:134;s:11:"blog_active";i:1;s:24:"blog_last_post_timestamp";i:1401608220;}i:9;O:8:"stdClass":26:{s:7:"post_id";i:30488;s:8:"post_url";s:49:"http://smehio.blogspot.com/2014/06/blog-post.html";s:10:"post_title";s:77:"العالم ينفجر، وهذه الأسباب (الحلقة الأولى)";s:10:"post_image";s:0:"";s:12:"post_excerpt";s:123:"-
